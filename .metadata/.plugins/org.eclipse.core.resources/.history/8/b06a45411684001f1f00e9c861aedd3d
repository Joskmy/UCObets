package co.edu.uco.ucobet.generales.init.domain.state;

import java.util.UUID;

import co.edu.uco.ucobet.generales.init.domain.Domain;
import co.edu.uco.ucobet.generales.init.domain.country.CountryDomain;

public class StateDomain extends Domain {
	private String name;
	private CountryDomain country;
	
	public StateDomain(final UUID id, final String name, final CountryDomain country) {
		super(id);
		setName(name);
		this.country = country;
	}

	public final String getName() {
		return name;
	}

	private final void setName(final String name) {
		this.name = name;
	}

	
	public final CountryDomain getCountry() {
		return country;
	}

	
	public final void setCountry(CountryDomain country) {
		this.country = country;
	}
	
	
	
	
}
